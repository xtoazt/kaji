{
  "name": "kaji",
  "version": "1.0.0",
  "description": "AI-powered ChromeOS vulnerability research tool",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server/index.ts",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "cd client && npm run build",
    "vercel-build": "npm run build:client && cp -r client/build/* ./",
    "start": "node dist/server.js",
    "install:all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.17",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.5",
    "@types/winston": "^2.4.4",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "chromeos",
    "vulnerability",
    "security",
    "ai",
    "exploit"
  ],
  "author": "Kaji Team",
  "license": "MIT"
}
